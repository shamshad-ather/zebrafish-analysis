    "# Assigns a quadrant (1-4) to each fish based on its coordinates.\n",
    "def assign_quadrants(df, num_fish, tank_width_cm, tank_height_cm, prefix):\n",
    "    center_x = tank_width_cm / 2\n",
    "    center_y = tank_height_cm / 2\n",
    "    width_quadrant = tank_width_cm / 2\n",
    "    height_quadrant = tank_height_cm / 2\n",
    "    for i in range(1, num_fish + 1):\n",
    "        df[f'{prefix}quadrant{i}'] = df.apply(\n",
    "            lambda row: determine_quadrant(row[f'{prefix}x{i}'], row[f'{prefix}y{i}'], center_x, center_y, width_quadrant, height_quadrant),\n",
    "            axis=1\n",
    "        )\n",
    "    return df\n",
    "\n",
    "# Determines the quadrant for a given x,y coordinate.\n",
    "def determine_quadrant(x, y, center_x, center_y, width_quadrant, height_quadrant):\n",
    "    if x <= center_x and y >= center_y:\n",
    "        return 1  # Upper Left\n",
    "    elif x > center_x and y >= center_y:\n",
    "        return 2  # Upper Right\n",
    "    elif x <= center_x and y < center_y:\n",
    "        return 3  # Lower Left\n",
    "    else:\n",
    "        return 4  # Lower Right\n",
    "\n",
    "# Calculates the time spent in each quadrant for each fish.\n",
    "def calculate_time_in_quadrants(df, num_fish, fps=30, prefix=''):\n",
    "    time_per_frame = 1 / fps\n",
    "    quadrant_times = {}\n",
    "    for i in range(1, num_fish + 1):\n",
    "        quadrant_times[f'fish{i}'] = {\n",
    "            1: 0, 2: 0, 3: 0, 4: 0\n",
    "        }\n",
    "        quadrant_counts = df[f'{prefix}quadrant{i}'].value_counts() # Counts the number of frames per quadrant\n",
    "        for quadrant, count in quadrant_counts.items():\n",
    "          quadrant_times[f'fish{i}'][quadrant] = count * time_per_frame  # Convert to seconds\n",
    "    return quadrant_times"
